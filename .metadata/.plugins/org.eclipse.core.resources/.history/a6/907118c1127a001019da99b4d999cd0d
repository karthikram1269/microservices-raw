package com.wipro.gp.services;

import com.wipro.gp.entities.MedicalRecord;
import org.springframework.web.multipart.MultipartFile;

import java.io.IOException;
import java.util.List;

public interface MedicalRecordService {
    MedicalRecord addRecord(MedicalRecord record);
    List<MedicalRecord> getRecordsByPatientId(Long patientId);
    MedicalRecord uploadFile(Long patientId, MultipartFile file) throws IOException;
}
